# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# DigitalOcean Spaces Configuration
EXPO_PUBLIC_DO_SPACES_ENDPOINT=https://your-region.digitaloceanspaces.com
EXPO_PUBLIC_DO_SPACES_BUCKET=your_bucket_name
EXPO_PUBLIC_DO_ACCESS_KEY=your_access_key
EXPO_PUBLIC_DO_SECRET_KEY=your_secret_key

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here
